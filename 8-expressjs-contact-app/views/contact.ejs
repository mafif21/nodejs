<div class="my-4">
  <h1>Contact List</h1>

  <div class="mt-4">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Phone</th>
          <th scope="col">Action</th>
        </tr>
      </thead>
      <tbody>
        <!-- inisiasi -->
        <% contacts.forEach((contact, index) => { %>
        <tr>
          <th scope="row"><%= ++index %></th>
          <td><%= contact.name %></td>
          <td><%= contact.email %></td>
          <td><%= contact.phone %></td>
          <td>
            <a href="/detail" class="btn btn-primary">Detail</a>
          </td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </div>
</div>
